  <script src="/socket.io/socket.io.js"></script>
  <script>


      livingroombutton = 1;
      livingroomtvbutton = 1;
      kitchenbutton = 1;
      kitchensmallbutton = 1;
      bathroombutton = 1;
      bedroombutton = 1;
      hallbutton = 1;

      var socket = io();

      $(document).ready(function() {

          $("#livingbutt").click( function() {
              livingroomBS();
          });
          $("#livingtvbutt").click( function() {
              livingroomtvBS();
          });
          $("#kitchenbutt").click( function() {
              kitchenBS();
          });
          $("#kitchensmallbutt").click( function() {
              kitchensmallBS();
          });
          $("#bathroombutt").click( function() {
              bathroomBS();
          });
          $("#bedroombutt").click( function() {
              bedroomBS();
          });
          $("#hallbutt").click( function() {
              hallBS();
          });

      });



      socket.on('fromserver', function (data) {
          if (data =='interval') {
//              livingroomBS();
          }
      });



      socket.on('PLClivingroom', function (data) {
          if (data == livingroombutton) {

          }else{ livingroomBS();}
      });
      socket.on('PLClivingroomtv', function (data) {
          if (data == livingroomtvbutton) {

          }else{ livingroomtvBS();}
      });
      socket.on('PLCkitchen', function (data) {
          if (data == kitchenbutton) {

          }else{ kitchenBS();}
      });
      socket.on('PLCkitchensmall', function (data) {
          if (data == kitchensmallbutton) {

          }else{ kitchensmallBS();}
      });
      socket.on('PLCbedroom', function (data) {
          if (data == bedroombutton) {

          }else{ bedroomBS();}
      });
      socket.on('PLCbathroom', function (data) {
          if (data == bathroombutton) {

          }else{ bathroomBS();}
      });
      socket.on('PLChall', function (data) {
          if (data == hallbutton) {

          }else{ hallBS();}
      });


      function livingroomBS(){
          livingroombutton++;
          if(livingroombutton == 1){
              $( "#livingbutt" ).removeClass('toggle-button-selected')
              socket.emit('livingroomstate',livingroombutton);
          }
          if(livingroombutton >1){
              $( "#livingbutt" ).addClass('toggle-button-selected')
              livingroombutton = 0;
              socket.emit('livingroomstate',livingroombutton);
          }
      }
      function livingroomtvBS(){
          livingroomtvbutton++;
          if(livingroomtvbutton == 1){
              $( "#livingtvbutt" ).removeClass('toggle-button-selected')
              socket.emit('livingroomtvstate',livingroomtvbutton);
          }
          if(livingroomtvbutton >1){
              $( "#livingtvbutt" ).addClass('toggle-button-selected')
              livingroomtvbutton = 0;
              socket.emit('livingroomtvstate',livingroomtvbutton);
          }
      }
      function kitchenBS(){
          kitchenbutton++;
          if(kitchenbutton == 1){
              $( "#kitchenbutt" ).removeClass('toggle-button-selected')
              socket.emit('kitchenstate',kitchenbutton);
          }
          if(kitchenbutton >1){
              $( "#kitchenbutt" ).addClass('toggle-button-selected')
              kitchenbutton = 0;
              socket.emit('kitchenstate',kitchenbutton);
          }
      }
      function kitchensmallBS(){
          kitchensmallbutton++;
          if(kitchensmallbutton == 1){
              $( "#kitchensmallbutt" ).removeClass('toggle-button-selected')
              socket.emit('kitchensmallstate',kitchensmallbutton);
          }
          if(kitchensmallbutton >1){
              $( "#kitchensmallbutt" ).addClass('toggle-button-selected')
              kitchensmallbutton = 0;
              socket.emit('kitchensmallstate',kitchensmallbutton);
          }
      }
      function bathroomBS(){
          bathroombutton++;
          if(bathroombutton == 1){
              $( "#bathroombutt" ).removeClass('toggle-button-selected')
              socket.emit('bathroomstate',bathroombutton);
          }
          if(bathroombutton >1){
              $( "#bathroombutt" ).addClass('toggle-button-selected')
              bathroombutton = 0;
              socket.emit('bathroomstate',bathroombutton);
          }
      }
      function bedroomBS(){
          bedroombutton++;
          if(bedroombutton == 1){
              $( "#bedroombutt" ).removeClass('toggle-button-selected')
              socket.emit('bedroomstate',bedroombutton);
          }
          if(bedroombutton >1){
              $( "#bedroombutt" ).addClass('toggle-button-selected')
              bedroombutton = 0;
              socket.emit('bedroomstate',bedroombutton);
          }
      }
      function hallBS(){
          hallbutton++;
          if(hallbutton == 1){
              $( "#hallbutt" ).removeClass('toggle-button-selected')
              socket.emit('hallstate',hallbutton);
          }
          if(hallbutton >1){
              $( "#hallbutt" ).addClass('toggle-button-selected')
              hallbutton = 0;
              socket.emit('hallstate',hallbutton);
          }
      }


